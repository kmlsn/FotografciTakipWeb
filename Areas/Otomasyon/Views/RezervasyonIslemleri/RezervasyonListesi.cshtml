            var surecGuncelleSecilenID = [];
            $("#tb-SozlesmeListesi").on('click', '.SurecGuncelle', function () {
                // Tüm işaretlerin kaldırılması
                var sec = $("input[type=checkbox]");
                $.each(sec, function (index, item) {
                    if (($(this).is(":disabled") == false)) {
                        $(item).prop("checked", true);
                        $(item).prop("checked", false);
                    }
                }); // Tüm süreçlerdeki tikler kaldırılıyor
                var idler = $(this).data("sureclerid");
                var sozlesmeid = $(this).data("sozlesmeid");
                
                surecGuncelleSecilenID = [];
               
                if (typeof (idler) === 'number') {
                    surecGuncelleSecilenID.push(idler);
                }
                else {
                    surecGuncelleSecilenID = idler.split(",");
                }
              
                for (var i = 0; i < surecGuncelleSecilenID.length; i++) {
                    var Secici = "#surecguncelle" + surecGuncelleSecilenID[i];
                    $(Secici).prop("checked", true);
                }
                $("#SurecleriGuncelleButon").val(sozlesmeid);
                $("#SurecleriGuncelleButon").data("SozlesmeMusteriId", $(this).data("musteriid"));
                $('#SurecGuncelleModal').modal('show');
            });

            var SurecGuncelleSurecListesi = $("#SureclerGuncelle :checkbox");
            SurecGuncelleSurecListesi.on("change", function () {
                SurecGuncelleSecilenSurecler();
            });
            function SurecGuncelleSecilenSurecler() {
                surecGuncelleSecilenID = [];
                SurecGuncelleSurecListesi.filter(":checked").each(function () {
                    surecGuncelleSecilenID.push($(this).data("surecid"));
                });
            }
            $("#SurecleriGuncelleButon").click(function () {

                var Sozlesmeid = $(this).val();
                var SurecData = new FormData();
                SurecData.append("surecGuncelleSecilenID", surecGuncelleSecilenID);
                SurecData.append("Sozlesmeid", Sozlesmeid);
                SurecData.append("MusteriId", $(this).data("SozlesmeMusteriId"));
                $.ajax({
                    type: "POST",
                    datatype: 'json',
                    data: SurecData,
                    contentType: false,
                    processData: false,
                    beforeSend: function () {
                        Swal.fire(
                        {
                            title: "Süreçler Güncelleniyor!",
                            html: "<strong>Lütfen Bekleyin.</strong>",
                            onBeforeOpen: function onBeforeOpen() {
                                Swal.showLoading();
                            }
                        });
                    },
                    url: "/Otomasyon/RezervasyonIslemleri/SozlesmeSurecGuncelle/",
                    success: function (data) {

                        if (data.Sonuc == false) {
                            alertify.set('notifier', 'delay', 5);
                            alertify.set('notifier', 'position', 'top-center');
                            alertify.error(data.Mesaj);
                        }
                        else if (data.Sonuc == true) {
                            swal.close();
                            alertify.set('notifier', 'delay', 3);
                            alertify.set('notifier', 'position', 'top-center');
                            alertify.success(data.Mesaj);
                            dt.api().ajax.reload();
                            $('#SurecGuncelleModal').modal('hide');
                        }
                    },
                    error: function (err) {
                        alert("HATA");
                    }
                });
