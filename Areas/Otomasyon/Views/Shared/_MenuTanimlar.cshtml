@using FotografciTakipWeb.Models
@{
    Lisanslar lisans = (Lisanslar)Session["Lisans"];
    List<KullaniciYetki> ky = (List<KullaniciYetki>)Session["Yetkiler"];
    bool tanimlaryetki = ky.FirstOrDefault(x => x.SayfaId == 9 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 9 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool cekimpaketleriyetki = ky.FirstOrDefault(x => x.SayfaId == 74 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 74 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool gelirgiderturleriyetki = ky.FirstOrDefault(x => x.SayfaId == 75 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 75 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool gunlukiskategorileriyetki = ky.FirstOrDefault(x => x.SayfaId == 76 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 76 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool personelgorevleriyetki = ky.FirstOrDefault(x => x.SayfaId == 77 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 77 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool rehbergruplariyetki = ky.FirstOrDefault(x => x.SayfaId == 78 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 78 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool rezervasyonekhizmetleriyetki = ky.FirstOrDefault(x => x.SayfaId == 79 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 79 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool rezervasyonturleriyetki = ky.FirstOrDefault(x => x.SayfaId == 80 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 80 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool emailmetinleriyetki = ky.FirstOrDefault(x => x.SayfaId == 81 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 81 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool smsmetinleriyetki = ky.FirstOrDefault(x => x.SayfaId == 82 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 82 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool sozlesmesartlariyetki = ky.FirstOrDefault(x => x.SayfaId == 83 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 83 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool surecleryetki = ky.FirstOrDefault(x => x.SayfaId == 84 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 84 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool tatilgunleriyetki = ky.FirstOrDefault(x => x.SayfaId == 85 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 85 && x.Sil == false && x.Aktif == true).SayfaYetki;
    bool zamandilimleriyetki = ky.FirstOrDefault(x => x.SayfaId == 86 && x.Sil == false && x.Aktif == true) == null ? false : ky.FirstOrDefault(x => x.SayfaId == 86 && x.Sil == false && x.Aktif == true).SayfaYetki;
}
@{
    string ustmenuclass = ViewBag.UstMenu == "Tanımlar" ? "active open" : "";
    string altmenucekimpaketleri = ViewBag.AltMenu == "Çekim Paketleri" ? "active" : "";
    string altmenugelirgiderturleri = ViewBag.AltMenu == "Gelir Gider Türleri" ? "active" : "";
    string altmenugunlukiskategorileri = ViewBag.AltMenu == "Günlük İş Kategorileri" ? "active" : "";
    string altmenupersonelgorevleri = ViewBag.AltMenu == "Personel Görevleri" ? "active" : "";
    string altmenurehbergruplari = ViewBag.AltMenu == "Rehber Grupları" ? "active" : "";
    string altmenurezervasyonekhizmetleri = ViewBag.AltMenu == "Rezervasyon Ek Hizmetleri" ? "active" : "";
    string altmenurezervasyonturleri = ViewBag.AltMenu == "Rezervasyon Türleri" ? "active" : "";
    string altmenuemailmetinleri = ViewBag.AltMenu == "Email Metinleri" ? "active" : "";
    string altmenusmsturleri = ViewBag.AltMenu == "SMS Metinleri" ? "active" : "";
    string altmenusozlesmesartlari = ViewBag.AltMenu == "Sözleşme Şartları" ? "active" : "";
    string altmenusurecler = ViewBag.AltMenu == "Süreçler" ? "active" : "";
    string altmenutatilgunleri = ViewBag.AltMenu == "Tatil Günleri" ? "active" : "";
    string altmenuzamandilimleri = ViewBag.AltMenu == "Zaman Dilimleri" ? "active" : "";
}
@if (tanimlaryetki)
{
    <li class=@ustmenuclass>
        <a href="#" title="Tanımlar" data-filter-tags="tanımlar">
            <i class="fal fa-info-circle"></i>
            <span class="nav-link-text" data-i18n="nav.tanimlar">Tanımlar</span>
        </a>
        <ul>
            @if (lisans.Tanimlar_CekimPaketleri)
            {
                if (cekimpaketleriyetki)
                {
                    <li class=@altmenucekimpaketleri>
                        <a href="~/Otomasyon/Tanimlar/CekimPaketleri" title="Çekim Paketleri" data-filter-tags="tanımlar çekim paketleri">
                            <span class="nav-link-text" data-i18n="nav.tanimlar_cekim_paketleri">Çekim Paketleri</span>
                        </a>
                    </li>
                }
            }
            @if (lisans.Tanimlar_GelirGiderTurleri)
            {
                if (gelirgiderturleriyetki)
                {
                    <li class=@altmenugelirgiderturleri>
                        <a href="~/Otomasyon/Tanimlar/GelirGiderTurleri" title="Gelir/Gider Türleri" data-filter-tags="tanımlar gelir gider türleri">
                            <span class="nav-link-text" data-i18n="nav.tanimlar_gelir_gider_turleri">Gelir/Gider Türleri</span>
                        </a>
                    </li>
