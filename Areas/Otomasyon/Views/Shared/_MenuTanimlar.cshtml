                        </a>
                    </li>
                }
            }
            @if (lisans.Tanimlar_EmailMetinleri)
            {
                if (emailmetinleriyetki)
                {
                    <li class=@altmenuemailmetinleri>
                        <a href="~/Otomasyon/Tanimlar/EmailMetinleri" title="Email Metinleri" data-filter-tags="tanımlar email metinleri">
